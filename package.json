    "eslintconfig",
    "eslint-plugin",
    "quotes",
    "smart-quotes",
    "curly-quotes"
  ],
  "author": "Younes Aassila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/younesaassila/eslint-plugin-curly-quotes/issues"
  },
  "homepage": "https://github.com/younesaassila/eslint-plugin-curly-quotes#readme",
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@types/eslint": "^8.44.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.17.1",
    "jest": "^29.6.2",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vue-eslint-parser": "^9.3.1"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  }
