    "@types/node": "^18.18.6",
    "@typescript-eslint/utils": "^6.8.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vue-eslint-parser": "^9.3.2"
  },
  "peerDependencies": {
    "eslint": "^8.51.0"
  }
